<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleWhiteProgressView">
        <attr name="backWidth" format="dimension" />    <!--背景圆环宽度-->
        <attr name="progWidth" format="dimension" />    <!--进度圆环宽度-->
        <attr name="backColor" format="color" />        <!--背景圆环颜色-->
        <attr name="progColor" format="color" />        <!--进度圆环颜色-->
        <attr name="progStartColor" format="color" />   <!--进度圆环开始颜色-->
        <attr name="progFirstColor" format="color" />   <!--进度圆环结束颜色-->
        <attr name="progress" format="integer" />       <!--圆环进度-->
    </declare-styleable>
    <declare-styleable name="CircleProgressBar">
        <!--底部颜色-->
        <attr name="color_bg" format="color"/>
        <!--进度颜色-->
        <attr name="color_progress" format="color"/>
        <!--进度条宽度-->
        <attr name="width_stroke" format="float"/>
        <!--绘圆与边界距离 范围 0f+ 不宜太大，最佳显示效果即可 -->
        <attr name="distance_boundary" format="float"/>
        <!--进度条初始进度 范围 0f-360f -->
       <!-- <attr name="progress" format="float"/>-->
    </declare-styleable>
</resources>